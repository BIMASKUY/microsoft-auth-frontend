# Environment Variables for Microsoft OAuth
# Copy this file to .env.local and fill in your actual values

# Your Azure AD Application (Client) ID
NEXT_PUBLIC_CLIENT_ID=your_client_id_here

# Your Azure AD Tenant ID (or use 'common' for multi-tenant)
NEXT_PUBLIC_TENANT_ID=your_tenant_id_here

# Your application's redirect URI (should match what's configured in Azure AD)
NEXT_PUBLIC_REDIRECT_URI=http://localhost:3000
